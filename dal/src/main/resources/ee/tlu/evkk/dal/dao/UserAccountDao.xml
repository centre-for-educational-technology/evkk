<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ee.tlu.evkk.dal.dao.UserAccountDao">

    <select id="findById" resultType="ee.tlu.evkk.dal.dto.UserAccount">
        SELECT *
          FROM core.user_account
         WHERE user_account_id = #{id};
    </select>

    <select id="findByEmailAddress" resultType="ee.tlu.evkk.dal.dto.UserAccount">
        SELECT *
          FROM core.user_account
         WHERE email_address = #{emailAddress};
    </select>


</mapper>